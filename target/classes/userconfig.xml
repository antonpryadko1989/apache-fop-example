<?xml version="1.0"?>
<fop version="1.0">
	<base>.</base>
	<renderers>
		<renderer mime="application/pdf">
			<filterList>
				<value>flate</value>
			</filterList>
			<fonts>
				<font metrics-file="VarelaRound-Regular.xml" kerning="yes"
					embed-url="VarelaRound-Regular.ttf">
					<font-triplet name="VarelaRound-Regular" style="normal"
						weight="normal" />
				</font>
				<font metrics-file="arial.xml" kerning="yes"
					embed-url="arial.ttf">
					<font-triplet name="Arial" style="normal"
						weight="normal" />
				</font>
			</fonts>
		</renderer>
	</renderers>
</fop>